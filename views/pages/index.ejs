<!DOCTYPE html>
<html lang="en">
    <head>
        <% include ../partials/head %>
    </head>
    <body class="-has-spa-header">
        <% include ../partials/header %>
        <main>
            <div class="gds-layout__container">
                <div class="gds-layout__row">
                    <!-- Sidebar -->
                    <aside>
                        <div class="gds-layout__column--lg-3 gds-layout__hidden-md-down">
                            <% Object.keys(sections).map((section) => { %>
                                <p class="gds-text--header-sm"><%= section %></p>
                                <% Object.keys(sections[section]).map((component) => { %>
                                    <p><%= component %></p>

                                    <% if (sections[section][component].children) { %>
                                        <% Object.keys(sections[section][component].children).map((childComponent) => { %>
                                            <p class="gds-text--body-sm"><%= childComponent %></p>
                                        <% }) %>
                                    <% } %>

                                <% }) %>
                            <% }) %>
                        </div>
                    </aside>

                    <!-- Themes section -->
                    <section>
                        <div class="gds-layout__column--lg-9 gds-layout__column--md-12">
                            <!-- -->
                        </div>
                    </section>

                    <!-- Subatomic section -->

                    <!-- Atoms section -->

                    <!-- Molecules section -->

                    <!-- Organisms section -->

                    <!-- Utilities section -->

                    <!-- Third-party section -->

                </div><!-- /row -->
            </div><!-- /container -->
        </main>
        <% include ../partials/footer %>
    </body>
</html>
